{
    "name": "sitec/siravel",
    "description": "Add to any Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "ricardo@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "sitec/laracogs": "dev-master",
        "illuminate/support": "5.4.*",

        "prettus/l5-repository": "2.6.*",

        "devfactory/minify": "1.0.*",
        "spatie/laravel-analytics": "^1.4",
        "aws/aws-sdk-php": "^3.18",
        "league/flysystem-aws-s3-v3": "^1.0",
        "graham-campbell/markdown": "^7.1",
        "intervention/imagecache": "^2.3",
        "stichoza/google-translate-php": "^3.2",
        "donatj/phpuseragentparser": "^0.5.3",

        "laracasts/flash": "^2.0.0",
        "laravelcollective/html": "^5.2.0",

        "laravel/socialite": "^2.0",
        "google/recaptcha": "~1.1",
        "predis/predis": "^1.1",
        "inani/larapoll": "^1.0",

        "jlapp/swaggervel": "dev-master",
        "doctrine/dbal": "~2.3",
        "amranidev/scaffold-interface": "^1.7",

        "yab/crypto": "^1.0"

    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.4.*|3.4-dev",

        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpspec/phpspec": "~3.0",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Sitec\\Siravel\\": "src/"
        },
        "files": [
            "src/Helpers/blade.php",
            "src/Helpers/general.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://ricardosierra@bitbucket.org/sierratecnologia/laracogs.git"
        }
    ]
}
